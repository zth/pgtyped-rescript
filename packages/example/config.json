{
  "transforms": [
    {
      "mode": "sql",
      "include": "**/*.sql",
      "emitTemplate": "{{dir}}/{{name}}__sql.res"
    }    
  ],
  "typesOverrides": {
    "date": {
      "return": "string"
    },
    "int8": "bigint"    
  },
  "srcDir": "./src/",
  "dbUrl": "postgres://postgres:password@localhost/postgres"
}
